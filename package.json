{
  "private": true,
  "name": "vue-rollup-boilerplate",
  "version": "1.0.0",
  "description": "A simple boilerplate Vue.js using Rollup",
  "author": "",
  "scripts": {
    "build": "rollup -c",
    "dev": "npm run serve",
    "postinstall": "husky install",
    "serve": "rollup -c -w",
    "start": "npm run serve"
  },
  "dependencies": {
    "@vue/composition-api": "1.3.0",
    "vue": "^2.6.14",
    "vue-i18n": "8.26.5",
    "vue-router": "3.5.3",
    "vuex": "^3.6.2"
  },
  "peerDependencies": {
    "postcss": "^8.3.5"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "3.1.8",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-image": "2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-replace": "3.0.0",
    "cssnano": "5.0.8",
    "esbuild": "0.13.10",
    "generate-source-map": "^0.0.5",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "postcss": "8.3.11",
    "postcss-import": "^14.0.2",
    "postcss-nested": "5.0.6",
    "postcss-preset-env": "^6.7.0",
    "postcss-sort-media-queries": "4.1.0",
    "prettier": "2.4.1",
    "prettier-package-json": "^2.6.0",
    "rollup": "2.58.3",
    "rollup-plugin-esbuild": "4.6.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "4.0.1",
    "rollup-plugin-require-context": "^1.0.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "rollup-plugin-vue": "^5.1.9",
    "unplugin-vue2-script-setup": "0.6.12",
    "vue-template-compiler": "^2.6.14"
  },
  "lint-staged": {
    "*.{js,json,vue},!dist/*.js": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json  --write",
      "git add"
    ]
  }
}
